					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('antarctica_expedition') )]"
						size = { 150 150 }

						texture = "gfx\antarctica_expedition.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('british_raj_decision') )]"
						size = { 150 150 }

						texture = "gfx\british_raj_decision.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('canada_unite_can') )]"
						size = { 150 150 }

						texture = "gfx\canada_unite_can.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('canada_unite_gbr') )]"
						size = { 150 150 }

						texture = "gfx\canada_unite_gbr.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('australia_unite_aus') )]"
						size = { 150 150 }

						texture = "gfx\australia_unite_aus.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('australia_unite_gbr') )]"
						size = { 150 150 }

						texture = "gfx\australia_unite_gbr.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('suez_survey_decision') )]"
						size = { 150 150 }

						texture = "gfx\suez_survey_decision.png"
						parentanchor = left|hcenter
						widgetanchor = left|hcenter
					}

					icon = {
						visible = "[ObjectsEqual( Decision.GetName, Localize('stop_suez_survey_decision') )]"
						size = { 150 150 }

